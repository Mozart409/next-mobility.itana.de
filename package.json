{
  "name": "next-mobility.itana.de",
  "version": "0.6.20",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:next": "next dev -H 0.0.0.0",
    "dev:path": "pathpida --ignorePath .gitignore --watch",
    "build2": "next build",
    "build": "pathpida --ignorePath .gitignore && next build",
    "postbuild": "node ./scripts/generate-sitemap.mjs",
    "start": "next start",
    "lint": "eslint --fix .",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx}' && eslint --fix .",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "spellcheck": "spellchecker --config .spellcheckerrc.json",
    "prod": "yarn run build && yarn run start"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.5",
    "@heroicons/react": "^1.0.6",
    "@sendgrid/mail": "^7.7.0",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/line-clamp": "^0.4.0",
    "@tailwindcss/typography": "^0.5.2",
    "@xstate/inspect": "^0.7.0",
    "@xstate/react": "^3.0.0",
    "autoprefixer": "^10.4.7",
    "classnames": "^2.3.1",
    "cors": "^2.8.5",
    "date-fns": "^2.28.0",
    "final-form": "^4.20.7",
    "isomorphic-dompurify": "^0.20.0",
    "next": "12.2.0",
    "next-pwa": "^5.5.4",
    "postcss": "^8.4.14",
    "preact": "^10.8.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-final-form": "^6.5.9",
    "react-final-form-listeners": "^1.0.3",
    "react-hot-toast": "^2.2.0",
    "react-iframe": "^1.8.0",
    "sharp": "^0.30.7",
    "tailwindcss": "^3.1.4",
    "webpack": "^5.73.0",
    "xstate": "^4.32.1",
    "zod": "^3.17.3"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.10.0",
    "@testing-library/cypress": "^8.0.3",
    "@types/cors": "^2.8.12",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "cy-view": "^1.0.11",
    "cypress": "10.3.0",
    "eslint": "^8.18.0",
    "eslint-config-next": "^12.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.1.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "globby": "^13.1.2",
    "husky": ">=6",
    "lint-staged": ">=13.0.3",
    "npm-run-all": "^4.1.5",
    "pathpida": "^0.18.1",
    "prettier": "^2.7.1",
    "spellchecker-cli": "^4.11.0",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --fix"
  }
}
